{
  "App": {
    "KptCook": {
      "ApiUrl": "https://mobile.kptncook.com",
      "ApiKey": "6q7QNKy-oIgk-IMuWisJ-jfN7s6",
      "AccessToken": "a4b31348-824b-49a9-afec-8717e47076b1"
    },
    "Mealie": {
      "ApiUrl": "http://tripoli:9926/api",
      "Username": "changeme@example.com",
      "Password": "MyPassword"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Settings.Configuration",
      "Serilog.Aggregating.Filter"
    ],
    "Filter": [
      {
        "Name": "UniqueOverSpan",
        "Args": {
          "expression": "1 = 1",
          "span": "00:00:10"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:j}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  }
}
